{
  "compilerOptions": {
    "types": ["vite/client"],
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@assets/*": ["assets/*"],
      "@constants/*": ["constants/*"],
      "@contexts/*": ["contexts/*"],
      "@data/*": ["data/*"],
      "@fonts/*": ["assets/fonts/*"],
      "@hooks/*": ["hooks/*"],
      "@images/*": ["assets/images/*"],
      "@langs/*": ["langs/*"],
      "@layouts/*": ["components/layouts/*"],
      "@modals/*": ["components/modals/*"],
      "@modules/*": ["components/modules/*"],
      "@pages/*": ["pages/*"],
      "@services/*": ["services/*"],
      "@styles/*": ["assets/styles/*"],
      "@utils/*": ["utils/*"]
    }
  }
}